{
  "name": "ember-cli-addon-docs-yuidoc",
  "version": "1.0.0",
  "description": "YUIDoc plugin for ember-cli-addon-docs",
  "keywords": [
    "ember-addon",
    "ember-cli-addon-docs-plugin"
  ],
  "repository": "https://github.com/ember-learn/ember-cli-addon-docs-yuidoc",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:js": "eslint ./*.js config lib tests node-tests",
    "start": "ember serve",
    "test": "mocha node-tests",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-caching-writer": "^3.0.3",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "fs-extra": "^9.0.1",
    "json-api-serializer": "^2.6.0",
    "lodash": "^4.17.20",
    "yuidocjs": "^0.10.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.1.2",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^11.4.2",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "mocha": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "release-it": "^14.2.0",
    "release-it-lerna-changelog": "^3.0.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
